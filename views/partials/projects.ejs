<% projects.forEach(function(proj) { %>
<h4 class="small font-weight-bold"><%= proj.name %><span class="float-right"><%= proj.progress %>%</span></h4>
<div class="progress mb-4">
    <div class="progress-bar bg-success" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100" style="width: 100%;" id="progress">
        <span class="sr-only"><%= proj.progress %></span></div>
</div>
<script>
    var breite = "<%= proj.progress %>" + "%";
    var rnd = Math.random();
    document.getElementById("progress").id = rnd;
    document.getElementById(rnd).style.width = breite;
</script>
<% }); %>